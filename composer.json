{
    "name": "spryker/merchant-user-auth-gui-page",
    "type": "library",
    "description": "MerchantUserAuthGuiPage module",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "spryker/auth": "^1.0.0 || ^2.0.0 || ^3.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/messenger": "^3.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/zed-ui": "^0.2.1"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "abandoned": "spryker/authentication-merchant-portal-gui"
}
